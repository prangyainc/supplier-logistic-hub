<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<P13nDialog showReset="true" showResetEnabled="true" ok="onOK" cancel="onCancel" reset="onReset" class="sapUiSizeCompact">
		<panels>
			<P13nColumnsPanel items="{path: 'p13Dialog>/ColumnCollection'}" columnsItems="{path: 'p13Dialog>/ColumnCollection'}">
				<items>
					<P13nItem columnKey="{p13Dialog>columnKey}" text="{p13Dialog>columnKey}"/>
				</items>
				<columnsItems>
					<P13nColumnsItem columnKey="{p13Dialog>columnKey}" visible="{p13Dialog>visible}"/>
				</columnsItems>
			</P13nColumnsPanel>
		</panels>
	</P13nDialog>
</core:FragmentDefinition>